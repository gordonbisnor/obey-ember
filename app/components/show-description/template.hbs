
{{#each show.artists as |artist| }}
  <div class="tc upcase f2 lh-solid mb2" data-artist-id="{{artist.id}}">
    {{#link-to 'artist' artist class="artist-name"}}
      {{artist.name}}
    {{/link-to}}
  </div>
{{/each}}

<div class="mt2 f4  upcase tc">
